    "Port": 5672
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost",
      "https://localhost",
      "http://localhost:3000",
      "http://localhost:5200",
      "http://localhost:5000"
    ]
  },
  "Swagger": {
    "ApiTitle": "Booking Service API",
    "ApiVersion": "v1",
    "ApiDescription": "Enterprise-level API for booking management with event-driven architecture"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "BookingService",
    "ServiceVersion": "1.0.0",
