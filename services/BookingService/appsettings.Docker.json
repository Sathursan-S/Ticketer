{
  "ConnectionStrings": {
    "PostgresSQL": "Host=postgres;Port=5432;Database=BookingServiceDb;Username=postgres;Password=postgres"
  },
  "UseInMemoryDatabase": false,
  "MongoDB": {
    "ConnectionString": "mongodb://mongodb:27017",
    "DatabaseName": "BookingServiceDb",
    "BookingsCollectionName": "Bookings"
  },
  "RabbitMq": {
    "Host": "rabbitmq",
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": 5672
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "DetailedErrors": true,
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "BookingService",
    "EnableConsoleExporter": true,
    "Jaeger": {
      "Enabled": true,
      "AgentHost": "jaeger",
      "AgentPort": 6831,
      "MaxPayloadSizeInBytes": 4096,
      "ExportProcessorType": "Batch"
    }
  }
}
